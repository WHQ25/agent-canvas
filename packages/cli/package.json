{
  "name": "@agent-canvas/cli",
  "version": "0.9.1",
  "description": "CLI tool for Agent Canvas - Excalidraw interface for AI agents",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/WHQ25/agent-canvas.git",
    "directory": "packages/cli"
  },
  "keywords": [
    "excalidraw",
    "canvas",
    "ai",
    "agent",
    "cli",
    "drawing"
  ],
  "bin": {
    "agent-canvas": "./bin/agent-canvas.js"
  },
  "main": "dist/index.js",
  "type": "module",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "dev": "bun run src/index.ts",
    "build": "tsc && mkdir -p ./dist/static && cp -r ../web-app/dist/* ./dist/static/",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "link:dev": "bash -c 'ln -sf \"$(pwd)/bin/agent-canvas.js\" \"$(dirname $(which agent-canvas))/agent-canvas-dev\" && echo \"Linked: agent-canvas-dev -> $(pwd)/bin/agent-canvas.js\"'",
    "unlink:dev": "bash -c 'rm -f \"$(dirname $(which agent-canvas))/agent-canvas-dev\" && echo \"Unlinked agent-canvas-dev\"'"
  },
  "dependencies": {
    "@toon-format/toon": "^2.1.0",
    "commander": "^13.0.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@agent-canvas/web-app": "workspace:*",
    "@types/ws": "^8.5.14",
    "typescript": "^5.7.3",
    "vitest": "^4.0.18"
  }
}
